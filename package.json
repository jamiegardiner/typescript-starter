{
	"name": "typescript-starter",
	"version": "2.0.0",
	"description": "A starter project for TypeScript",
	"main": "index.ts",
	"engines": {
		"node": ">=22.2.0",
		"pnpm": ">=9.1.4"
	},
	"scripts": {
		"prepare": "husky",
		"preinstall": "npx only-allow pnpm",
		"test": "vitest",
		"test:coverage": "vitest --coverage",
		"test:types": "tsc --noEmit",
		"format": "biome check --write",
		"format:check": "biome format",
		"lint:check": "biome lint"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jamiegardiner/typescript-starter.git"
	},
	"author": "Jamie Gardiner",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jamiegardiner/typescript-starter/issues"
	},
	"homepage": "https://github.com/jamiegardiner/typescript-starter#readme",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@tsconfig/strictest": "^2.0.5",
		"@types/node": "^22.7.7",
		"@types/ramda": "^0.30.2",
		"@vitest/coverage-v8": "2.1.3",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"vitest": "^2.1.3"
	},
	"dependencies": {
		"funfix": "^7.0.1",
		"ramda": "^0.30.1"
	}
}
