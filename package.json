{
	"name": "typescript-starter",
	"version": "2.0.0",
	"description": "A starter project for TypeScript",
	"main": "index.ts",
	"scripts": {
		"prepare": "husky",
		"preinstall": "npx only-allow pnpm",
		"test": "vitest",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest --coverage",
		"test:types": "tsc --noEmit",
		"lint": "biome lint --write",
		"lint:check": "biome lint",
		"format": "biome format --write",
		"format:check": "biome format"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jamiegardiner/typescript-starter.git"
	},
	"author": "Jamie Gardiner",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jamiegardiner/typescript-starter/issues"
	},
	"homepage": "https://github.com/jamiegardiner/gift-tracker#readme",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@commitlint/cli": "^17.8.1",
		"@commitlint/config-conventional": "^17.8.1",
		"@tsconfig/strictest": "^2.0.5",
		"@types/node": "^18.19.57",
		"@types/ramda": "^0.28.25",
		"@vitest/coverage-v8": "2.1.3",
		"husky": "^9.1.6",
		"lint-staged": "^13.3.0",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.2",
		"typescript": "^4.9.5",
		"vitest": "^2.1.3"
	},
	"dependencies": {
		"funfix": "^7.0.1",
		"ramda": "^0.28.0"
	}
}
