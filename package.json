{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "A starter project for TypeScript",
  "main": "index.ts",
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:types": "tsc --noEmit",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --ignore-unknown --write --cache ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiegardiner/typescript-starter.git"
  },
  "author": "Jamie Gardiner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamiegardiner/typescript-starter/issues"
  },
  "homepage": "https://github.com/jamiegardiner/gift-tracker#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@tsconfig/node-lts-strictest": "^18.12.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/ramda": "^0.28.20",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "funfix": "^7.0.1",
    "ramda": "^0.28.0"
  }
}
